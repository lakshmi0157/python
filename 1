def main():
    # Initialize the dictionary with keys and values
    fruits_dict = {
        'apple': 5,
        'orange': 10,
        'banana': 12
    }
    
    # Print the contents of the dictionary
    print("Dictionary contents:")
    for key, value in fruits_dict.items():
        print(f"{key}: {value}")

if __name__ == "__main__":
    main()
